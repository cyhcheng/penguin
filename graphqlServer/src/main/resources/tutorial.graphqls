type User {
    id: String !
    fullname: String !
    account: String !
    password: String !
    email: String !
    mobile: String !
    gender: Gender
    description: String
    createTime: String
}

enum Gender {
    Male
    Female
}

type Query {
    listUsers: [User]
    getUser(account: String): User
}


type Mutation {
    createUser(fullname: String !, account: String !, email: String !, mobile: String !): User
}